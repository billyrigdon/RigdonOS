{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"allowJs": true,
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"moduleResolution": "node",
		"baseUrl": "./",
		"typeRoots": ["node_modules/@types"],
		"esModuleInterop": true
	},
	"include": ["./**/*"],
	"exclude": ["node_modules", "build","dist"]
}
